[
  {
    "description": "这是一个带headers的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "zl",
        "age": "18"
      }
    },
    "response": {
      "json":{
        "status": "1",
        "zl": "success"
      }
    }
  }
]